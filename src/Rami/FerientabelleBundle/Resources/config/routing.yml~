    pattern:  /start
    defaults: { _controller: RamiFerientabelleBundle:Default:start }

rami_ferientabelle_start:
    pattern:  /stats
    defaults: { _controller: RamiFerientabelleBundle:Default:stats }
    
rami_ferientabelle_profile:
    pattern:  /{id}/{public}
    defaults: { _controller: RamiFerientabelleBundle:Default:profile }
    
rami_ferientabelle_profile_addfriend:
    pattern:  /add/{id}/{public}
    defaults: { _controller: RamiFerientabelleBundle:Default:addFriendDirect }
    
rami_ferientabelle_account:
    pattern:  /{id}/{public}/{private}
    defaults: { _controller: RamiFerientabelleBundle:Default:account }
    
rami_ferientabelle_account_timeframe_delete:
    pattern:  /{id}/{public}/{private}/delete/{tf}
    defaults: { _controller: RamiFerientabelleBundle:Default:timeframeDelete }
    
