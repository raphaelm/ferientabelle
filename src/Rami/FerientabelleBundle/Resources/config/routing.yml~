    pattern:  /{id}/{public}/{private}/addfriend
    defaults: { _controller: RamiFerientabelleBundle:Default:addFriend }
    
rami_ferientabelle_account_deletefriend:
    pattern:  /{id}/{public}/{private}/deletefriend/{friend}
